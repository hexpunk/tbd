{
  "name": "tbd/app",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "lint": "phpstan analyze",
    "test": "phpunit"
  },
  "require": {
    "php": "^8.0",
    "monolog/monolog": "2.7.0",
    "nyholm/psr7": "1.5.1",
    "nyholm/psr7-server": "1.0.2",
    "php-di/php-di": "6.4.0",
    "php-di/slim-bridge": "3.2.0",
    "psr/container": "1.1.2",
    "slim/http": "1.2.0",
    "slim/slim": "4.10.0",
    "symfony/dotenv": "6.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.21",
    "phpstan/phpstan": "1.8.1",
    "psy/psysh": "0.11.7"
  }
}
